<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mridul.eventmanager">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/apppicturecircle10"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="Events" />
        <activity
            android:name=".GuestActivity"
            android:label="Events" />
        <activity
            android:name=".RegisterActivity"
            android:label="Register" />
        <activity
            android:name=".CreateEvent"
            android:label="Create Event" />
        <activity
            android:name=".ProfileActivity"
            android:label="Profile" />
        <activity
            android:name=".RemoveEvent"
            android:label="Remove Event" />
        <activity android:name=".DetailActivity" />
        <activity android:name=".RegisterActivity2" />
        <activity android:name=".OrganiserDetailActivity"></activity>
    </application>

</manifest>